!! rxvt-unicode
URxvt.termName: rxvt-unicode
URxvt.pointerBlank: true
URxvt.font: xft:Misc Tamsyn:pixelsize=17,xft:DejaVu Sans Mono:pixelsize=17
!URxvt.font: xft:Input:size=11
!URxvt.font: xft:Roboto Mono:size=10
!URxvt.letterSpace: 1
!URxvt.lineSpace: 1
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
URxvt.saveLines: 100000
URxvt.urgentOnBell: true
URxvt.keysym.C-Left: \033[1;5D
URxvt.keysym.C-Right: \033[1;5C
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.matcher.button: C-1
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.keysym.C-S-Down: resize-font:smaller
URxvt.keysym.C-S-Up: resize-font:bigger
URxvt.depth: 32
URxvt.foreground: #909090
URxvt.transparent: false
URxvt.shading: 20
URxvt.fading: 20
URxvt.iso14755: false
URxvt.intensityStyles: false
